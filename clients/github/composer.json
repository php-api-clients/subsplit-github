{
  "name": "api-clients/github",
  "description": "Non-Blocking first GitHub client",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "api-clients/github-common": "^0.3",
    "api-clients/contracts": "^0.1",
    "cebe/php-openapi": "dev-openapi-31 as 1.6.0",
    "eventsauce/object-hydrator": "^1.1",
    "league/openapi-psr7-validator": "^0.18",
    "react/async": "^4.0",
    "wyrihaximus/react-awaitable-observable": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Client\\GitHub\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2.13"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "wyrihaximus/composer-update-bin-autoload-path": true,
      "infection/extension-installer": true
    }
  },
"sugest": {
"api-clients/github-meta": "*","api-clients/github-apps": "*","api-clients/github-codes-of-conduct": "*","api-clients/github-emojis": "*","api-clients/github-dependabot": "*","api-clients/github-secret-scanning": "*","api-clients/github-activity": "*","api-clients/github-gists": "*","api-clients/github-gitignore": "*","api-clients/github-issues": "*","api-clients/github-licenses": "*","api-clients/github-markdown": "*","api-clients/github-orgs": "*","api-clients/github-actions": "*","api-clients/github-oidc": "*","api-clients/github-code-scanning": "*","api-clients/github-codespaces": "*","api-clients/github-packages": "*","api-clients/github-interactions": "*","api-clients/github-migrations": "*","api-clients/github-projects": "*","api-clients/github-repos": "*","api-clients/github-billing": "*","api-clients/github-teams": "*","api-clients/github-reactions": "*","api-clients/github-rate-limit": "*","api-clients/github-checks": "*","api-clients/github-dependency-graph": "*","api-clients/github-git": "*","api-clients/github-pulls": "*","api-clients/github-security-advisories": "*","api-clients/github-search": "*","api-clients/github-users": "*","api-clients/github-webhook": "*"},
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^5.0"
  }
}
